{
	"expo": {
		"name": "Flowbeat",
		"slug": "Flowbeat",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"scheme": "Flowbeat",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.tonjiji.Flowbeat"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-dev-client",
				{
					"launchMode": "most-recent"
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "732aeadc-daf5-48da-9466-3d1c9825c63a"
			}
		}
	}
}
